program karakterRahasia
kamus
x1,x2,x3,x4,x5: integer
y1,y2,y3,y4 : char

algoritma
input(x1,x2,x3,x4)
    y1 <- ((x1 + x2) mod 4096) >> 6 + 64
    y2 <- ((x2 + x3) mod 4096) >> 6 + 64
    y3 <- ((x3 + x4) mod 4096) >> 6 + 64
    y4 <- ((x4 + x5) mod 4096) >> 6 + 64
output(y1,y2,y3,y4)

endprogram