program toko

kamus 
barang1, barang2, barang3 : integer
harga1, harga2, harga3 : real

algoritma
input(barang1, barang2, barang 3)
    harga1 <- barang1 + barang1 * 0.05
    harga2 <- barang2 + barang2 * 0.05
    harga3 <- barang3 + barang3 * 0.05
output(harga1, harga2, harga3)

endprogram